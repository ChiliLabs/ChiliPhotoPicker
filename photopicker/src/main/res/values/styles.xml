<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TextAppearance.PhotoPicker.Button" parent="TextAppearance.AppCompat.Medium">
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textColor">?pickerControlColor</item>
        <item name="android:includeFontPadding">true</item>
        <item name="android:textSize">@dimen/textSizeMedium</item>
    </style>
    <style name="NavigationImageButton" parent="Widget.AppCompat.Toolbar.Button.Navigation">
        <item name="android:layout_width">56dp</item>
        <item name="android:layout_height">56dp</item>
        <item name="layout_constraintStart_toStartOf">parent</item>
        <item name="layout_constraintTop_toTopOf">parent</item>
    </style>
    <style name="ImageButton" parent="Widget.AppCompat.ImageButton">
        <item name="android:background">?attr/selectableItemBackgroundBorderless</item>
    </style>
</resources>